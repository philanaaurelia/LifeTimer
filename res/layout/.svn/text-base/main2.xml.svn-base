<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    android:id="@+id/widget54"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  >
  
  <Spinner 
        android:id="@+id/catSpinner"
        android:layout_width="250px"
        android:layout_height="wrap_content"
        android:layout_gravity = "center"
        android:prompt="@string/planet_prompt"
        android:layout_marginTop="10px"
    />
    
  <EditText
  		android:id="@+id/AddCatName"   
  		android:layout_height="wrap_content"     
   		android:layout_width="250px"
   		android:layout_gravity = "center"
   		android:visibility="gone"
  ></EditText>
  
  
  <HorizontalScrollView
    android:id="@+id/widget54"
    android:layout_width="250px"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:scrollbars="horizontal"
    android:fadeScrollbars="false"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
  

<RadioGroup android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:visibility="gone"
			android:orientation="horizontal"
			android:layout_marginBottom="10px"
			android:id="@+id/RadioColors">
			
	
	<RadioButton android:id="@+id/radioRed" 
				 android:checked="false"
				 android:width="51px"
				 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
				 android:button="@drawable/red_radio"
				 />
				 
	<RadioButton android:id="@+id/radioTur" 
				 android:checked="false"
				 android:width="51px"
				 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 android:button="@drawable/turquoise_radio"
                 />
                 
   <RadioButton android:id="@+id/radioPur" 
				 android:checked="false"
				 android:width="51px"
				 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
				 android:button="@drawable/purple_radio"
				 />
				 
	<RadioButton android:id="@+id/radioBlg" 
				 android:checked="false"
				 android:width="51px"
				 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 android:button="@drawable/bluegreen_radio"
                 />
		
	<RadioButton android:id="@+id/option5" 
				 android:checked="false"
				 android:width="51px"
				 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
				 android:button="@drawable/yellow_radio"
				 />

	<RadioButton android:id="@+id/option6" 
				 android:checked="false"
				 android:width="51px"
				 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 android:button="@drawable/pink_radio"
                 />
     
   <RadioButton android:id="@+id/radioLiG" 
				 android:checked="false"
				 android:button="@drawable/limegreen_radio"
				 android:width="51px"
                 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
				 />
				 
	<RadioButton android:id="@+id/option8" 
				 android:checked="false"
                 android:button="@drawable/slateblue_radio"
                 android:width="51px"
                 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 />
                 
    <RadioButton android:id="@+id/radioBlu" 
				 android:checked="false"
                 android:button="@drawable/blue_radio"
                 android:width="51px"
                 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 />
    <RadioButton android:id="@+id/radioGra" 
				 android:checked="false"
                 android:button="@drawable/gray_radio"
                 android:width="51px"
                 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 />
	<RadioButton android:id="@+id/radioPlu" 
				 android:checked="false"
                 android:button="@drawable/plum_radio"
                 android:width="51px"
                 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 />
	<RadioButton android:id="@+id/radioOra" 
				 android:checked="false"
                 android:button="@drawable/orange_radio"
                 android:width="51px"
                 android:layout_marginLeft="5px"
				 android:layout_marginRight="5px"
                 />
</RadioGroup>
</HorizontalScrollView>
  <Spinner 
        android:id="@+id/subcatSpinner"
        android:layout_height="wrap_content"
        android:layout_width="250px"
        android:layout_gravity = "center"
        android:prompt="@string/planet_prompt"
    />
	<EditText
   		android:id="@+id/AddSubCatName"        
   		android:layout_height="wrap_content"
   		android:layout_width="250px"
   		android:layout_gravity = "center"
   		android:visibility="gone"
	></EditText>
	
